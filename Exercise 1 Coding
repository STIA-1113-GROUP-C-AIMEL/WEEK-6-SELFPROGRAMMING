package selfProgramming;

public class Condition {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		// to declare
		int x = 2;
		int y = 3;
		
		if (x<y) {
			System.out.println("Is true that x less than y");
		}
		else
			System.out.println("Is false that x greater than y");
		
	}

}
